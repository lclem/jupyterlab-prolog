{
  "name": "jupyterlab-prolog",
  "version": "1.0.0-pre.1",
  "private": false,
  "description": "An experimental Prolog extension for JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "prolog"
  ],
  "homepage": "https://github.com/lclem/jupyterlab-prolog",
  "bugs": {
    "url": "https://github.com/lclem/jupyterlab-prolog/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Lorenzo Clemente <clementelorenzo@gmail.com>",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf,ts}",
    "lib/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf,ts}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lclem/jupyterlab-prolog.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "precommit": "lint-staged",
    "prettier": "prettier --write '*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.5",
    "@jupyterlab/apputils": "^1.0.5",
    "@jupyterlab/codemirror": "^1.0.5",
    "@jupyterlab/console": "^1.0.5",
    "@jupyterlab/docregistry": "^1.0.5",
    "@jupyterlab/notebook": "^1.0.5",
    "@jupyterlab/fileeditor": "^1.0.5",
    "@jupyterlab/markdownviewer": "^1.0.5",
    "@jupyterlab/services": "^4.0.5",
    "husky": "^1.3.1",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@types/codemirror": "~0.0.76",
    "codemirror-mode-prolog": "^1.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.1",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "~3.3.1"
  },
  "jupyterlab": {
    "extension": true
  },
  "discovery": {
    "kernel": [
      {
        "kernel_spec": {
          "language": "^prolog"
        },
        "base": {
          "name": "prolog-kernel"
        },
        "managers": [
          "pip"
        ]
      }
    ]
  },
  "lint-staged": {
    "**/*{.ts,.tsx,.css,.json,.md}": [
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "@types/react": "~16.4.13"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "lib"
  }
}
